{
    "name": "argo-trading",
    "description": "HTML5 interface for the OANDA platform",
    "version": "5.0.2",
    "main": "./main.js",
    "bin": {
        "argo-trading": "./bin/argo-trading",
        "argo-trading-standalone": "./bin/argo-trading-standalone"
    },
    "preferGlobal": true,
    "repository": "git@github.com:albertosantini/argo.git",
    "keywords": [
        "trading",
        "forex",
        "fxtrade",
        "oanda"
    ],
    "author": "Alberto Santini",
    "license": "MIT",
    "engines": {
        "node": ">=8.9.0"
    },
    "dependencies": {
        "angular": "^1.6.6",
        "angular-mocks": "^1.6.6",
        "async": "^2.6.0",
        "body-parser": "^1.18.2",
        "d3": "^4.11.0",
        "document-register-element": "^1.7.0",
        "express": "^4.16.2",
        "faye-websocket": "^0.11.1",
        "flic": "^1.3.7",
        "hyperhtml": "^2.1.2",
        "introspected": "^0.2.5",
        "limiter": "^1.1.2",
        "request": "^2.83.0",
        "tachyons": "^4.9.0",
        "techan": "^0.8.0"
    },
    "optionalDependencies": {
        "electron": "^1.7.6"
    },
    "scripts": {
        "test": "npm run lint && npm run karma",
        "start": "node src/server/app.js",
        "debug": "node --inspect src/server/app.js",
        "electron": "electron .",
        "lint": "eslint main.js karma.conf.js src/**/*.js",
        "karma": "karma start --single-run",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "rollup": "rollup -c",
        "rollup-spec": "rollup -c rollup.config.spec.js",
        "rollup-d3": "rollup -c rollup.config.d3-techan.js && uglifyjs build/d3-techan.js -c -m -o build/d3-techan.min.js"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "conventional-changelog-cli": "^1.3.5",
        "eslint": "^4.11.0",
        "eslint-plugin-angular": "^3.1.1",
        "karma": "^1.7.1",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-mocha": "^1.3.0",
        "mocha": "^4.0.1",
        "rollup": "^0.51.8",
        "rollup-plugin-node-resolve": "^3.0.0",
        "uglify-es": "^3.1.10"
    }
}
